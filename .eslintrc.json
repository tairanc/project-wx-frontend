{
  "env": {
    "browser": true
  },
  "extends": "airbnb",
// 暂时关闭规则
  "rules": {
    // allow paren-less arrow functions
    "arrow-parens": 0,
    // allow async-await
    "generator-star-spacing": 0,
    // allow debugger during development
    "no-debugger": 0,
    "eol-last": 0,
    "space-before-function-paren": 0,
    "no-extra-boolean-cast": 0,
    "indent":[0],
    "no-multi-spaces":[0],
    "quotes": [0],
    "space-before-blocks":[0],
    "spaced-comment":[0],
    "no-unused-vars":[0],
    "object-curly-spacing":[0],
    "no-trailing-spaces":[0],
    "key-spacing":[0],
    "comma-dangle":[0],
    "no-proto":[0],
    "wrap-iife": [0],
    "padded-blocks":[0],
    "max-len":[0],
    "quote-props":[0],
    "global-require": [0],
    "no-useless-escape": [0],
    "object-shorthand": [0],
    "consistent-return": [0],
    "prefer-template": [0],
    "semi": 0
  }
}